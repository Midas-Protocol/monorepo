{
  "name": "@midas-capital/sdk",
  "repository": "git://github.com:midas-protocol/contracts.git",
  "version": "0.1.14",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "description": "",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "files": [
    "/dist/",
    "/artifacts",
    "deployments.json"
  ],
  "scripts": {
    "build": "npm run setup && npm run typechain && npm run build:sdk ",
    "build:sdk": "tsc -p tsconfig-sdk.json && tsc -p tsconfig-cjs.json",
    "compile": "hardhat compile",
    "setup": "cd lib/contracts && forge build",
    "test": "hardhat test",
    "test:bsc": "FORK_URL=${FORK_URL_BSC} FORK_BLOCK_NUMBER=${FORK_BLOCK_NUMBER} FORK_CHAIN_ID=56 hardhat test",
    "export": "hardhat export --export-all ./deployments.json",
    "clean": "rm -rf dist",
    "prettier:ts": "prettier --write 'src/**/*.ts'",
    "prettier": "npm run prettier:ts",
    "typechain": "typechain --target ethers-v5 --out-dir './lib/contracts/typechain' './lib/contracts/out/*/**.json'",
    "deploy:simulate:bsc:mainnet": "FORK_URL_BSC=${FORK_URL_BSC} FORK_BLOCK_NUMBER=${FORK_BLOCK_NUMBER} FORK_CHAIN_ID=56 hardhat deploy --tags simulate",
    "deploy:bsc:mainnet": "hardhat --network bsc deploy --tags prod"
  },
  "keywords": [],
  "author": "Midas Capital",
  "license": "ISC",
  "devDependencies": {
    "@typechain/ethers-v5": "^10.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "bufferutil": "^4.0.6",
    "chai": "^4.3.6",
    "dotenv": "^16.0.0",
    "ts-node": "^10.7.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3",
    "utf-8-validate": "^5.0.9"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.6.1",
    "@ethersproject/abstract-provider": "^5.6.0",
    "@ethersproject/bytes": "^5.6.1",
    "@ethersproject/hardware-wallets": "^5.6.0",
    "@ethersproject/providers": "^5.6.5",
    "@keydonix/uniswap-oracle-sdk": "^1.0.7",
    "@keydonix/uniswap-oracle-sdk-adapter": "^1.0.7",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@tenderly/hardhat-tenderly": "^1.0.13",
    "@typechain/hardhat": "^6.0.0",
    "@types/bad-words": "^3.0.1",
    "axios": "^0.26.1",
    "bad-words": "^3.0.4",
    "chalk": "^4.1.2",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.2",
    "hardhat": "^2.9.2",
    "hardhat-abi-exporter": "^2.8.0",
    "hardhat-deploy": "^0.11.4",
    "hardhat-tracer": "^1.0.0-alpha.6",
    "lint-staged": "12.3.7",
    "prettier": "2.6.1"
  }
}
